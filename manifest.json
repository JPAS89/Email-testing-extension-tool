{
  "manifest_version": 3,
  "name": "TestSuiteX",
  "version": "3.0",
  "description": "TestSuiteX is your all-in-one QA toolâ€”audit emails, banners, and web creatives for accessibility, link health, and more",
  "permissions": [
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://mail.google.com/*",
    "*://outlook.live.com/*",
    "*://outlook.office.com/*",
    "https://studio.bypgd.com/*",
    "*://*/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "logo2.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "links.html", 
        "results.js"

      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}